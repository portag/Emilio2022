 */
public class ejercicio2 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int a,b,c;
		a=3;
		b=4;
		c=2*a*b;
		a=a+2;
		b=c-a;
		System.out.println("el valor es de"+" "+a);
		System.out.println("el valor es de"+" "+b);
		System.out.println("el valor es de"+" "+c);

	}

}
