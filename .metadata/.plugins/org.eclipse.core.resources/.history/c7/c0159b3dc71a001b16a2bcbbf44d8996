package IntroduccionProgramacion;

/**
 * @author emilio
 *
 */
public class ejercicio1 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int a,b,c;
		int caso1,caso2,caso3,caso4,caso5;
		a=6;
		b=2;
		c=3;
		caso1=(a*c)%c;
		caso2=a*b/c;
		caso3=(c%b)+(c*b);
		caso4=a%(a*b*c/(b+c));
		caso5=b*b+c-b*(a%b);
		System.out.println("el resultado de la operacion es"+caso1);
		System.out.println("el resultado de la operacion es"+caso2);
		System.out.println("el resultado de la operacion es"+caso3);
		System.out.println("el resultado de la operacion es"+caso4);
		System.out.println("el resultado de la operacion es"+caso5);
		

	}

}
